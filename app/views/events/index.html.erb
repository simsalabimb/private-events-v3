<h1>CHECK OUT ALL YOUR EVENTS BELOW</h1>
<br>

<h3>Logged In: <%= current_user.username %> </h3>

<table>
  <tr>
    <th>Title</th>
    <th>Event</th>
    <th>Date</th>
  </tr>
   <% @event.each do |event| %>
   <tr>
    <td><%= event.title %></td>
    <td> <%= event.description %></td>
    <td><%= event.date %></td>
  </tr>
   <% end %>
</table>

<% if false %>
<% @events.each do |event| %>
    <p><strong>EVENT: </strong></p>
    <b>title:</b> <%= event.title %><br>
    <b>event:</b>  <%= event.description %><br>
    <b>date:</b>  <%= event.date %><br>

<% end %>

<% end %>
