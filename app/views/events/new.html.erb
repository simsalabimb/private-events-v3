<h1>Sign Up</h1>
<%= form_for @event do |f|%>
  <%= f.label :title%><br>
  <%= f.text_field :title%><br><br>

  <%= f.label :description%><br>
  <%= f.text_area :description%><br>

  <%= f.label :date%><br>
  <%= f.datetime_select :date%><br><br>
  
  <%= f.label :location%><br>
  <%= f.text_field :location%><br><br>

  <%= f.submit %>
<%end%>
